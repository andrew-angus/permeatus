}

@article{liuUltrahigh2021,
  title = {Ultra-High Gas Barrier Composites with Aligned Graphene Flakes and Polyethylene Molecules for High-Pressure Gas Storage Tanks},
  author = {Liu, Guanjun and Yang, Fan and Liu, Wenbo and Bai, Yujiao and Han, Chuang and Jiao, Weicheng and Wang, Peipei and Wang, Rongguo},
  year = {2021},
