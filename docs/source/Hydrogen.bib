  isbn = {9781267804952},
  langid = {english},
  school = {The University of Nebraska - Lincoln},
  keywords = {Applied sciences,Fracture criteria,Green's function,Hdpe,Hydrogen gas,Parameters estimation,Permeation,Pure sciences},
  file = {/home/u1997128/Zotero/storage/WNIX32MC/Prachumchon - A study of HDPE in high pressure of hydrogen gas—m.pdf}
