  langid = {english},
  keywords = {composites,finite elements,graphene,machine learning,molecular dynamics},
  file = {/home/u1997128/Zotero/storage/XHMP5K6T/Chen and Gu - 2019 - Effect of Constituent Materials on Composite Perfo.pdf;/home/u1997128/Zotero/storage/YQQDME42/adts.html}
