  author = {Li, Yufan and Barzagli, Francesco and Liu, Peng and Zhang, Xiaoan and Yang, Zhao and Xiao, Min and Huang, Yangqiang and Luo, Xiao and Li, Chao'en and Luo, He'an and Zhang, Rui},
  year = {2023},
