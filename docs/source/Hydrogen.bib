@article{kulkarniDamage2021,
  title = {Damage Evolution in Polymer Due to Exposure to High-Pressure Hydrogen Gas},
  author = {Kulkarni, Shank S. and Choi, Kyoo Sil and Kuang, Wenbin and Menon, Nalini and Mills, Bernice and Soulami, Ayoub and Simmons, Kevin},
  year = {2021},
