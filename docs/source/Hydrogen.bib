@article{backensComparative2021,
  title = {Comparative Study of Thermoplastic Liner Materials with Regard to Mechanical and Permeation Barrier Properties before and after Cyclic Thermal Aging},
  author = {Backens, Simon and Siering, Jan and Schmidt, Stefan and Gl{\"u}ck, Nikolai and Fl{\"u}gge, Wilko},
  year = {2021},
