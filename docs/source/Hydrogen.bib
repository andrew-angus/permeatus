@article{monsalve-bravoMultiscale2020,
  title = {Multiscale Simulation of Gas Transport in Mixed-Matrix Membranes with Interfacial Polymer Rigidification},
  author = {{Monsalve-Bravo}, Gloria M. and Dutta, Ravi C. and Bhatia, Suresh K.},
  year = {2020},
  month = apr,
  journal = {Microporous and Mesoporous Materials},
